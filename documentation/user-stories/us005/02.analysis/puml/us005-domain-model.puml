@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

skinparam classAttributeIconSize 0

hide circle
hide methods

title Domain Model - US005

class ProductionManager {
}

class Order {
    id
    orderDate
    deliveryDate
    price
}

class Address {
    id
    street
    zipCode
    town
    country
}

enum ProcessState {
    state
}

class Product {
    id
    name
    description
    capacity
    size
    color
    price
}


ProductionManager "1" -- "1..*" Order : register >
Order "*" -- "1..*" Product : contains >
Order "*" -- "1" Address : delivered to >
Order "*" -- "1" ProcessState : has >

@enduml
